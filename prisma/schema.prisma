// prisma/schema.prisma
datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
  // uncomment next line if you use Prisma <5.10
  // directUrl = env("DATABASE_URL_UNPOOLED")
}

// schema.prisma
generator client {
  provider        = "prisma-client-js"
}

model Board {
  id String @id @default(uuid())
  title String
}